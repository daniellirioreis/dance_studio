<br>
   <div class="row">
		<div class="col-md-5">
			<%= f.input :name, input_html: {maxlength: 50}%>
		</div>
	</div>

   <div class="row">
		<div class="col-md-2">
			<%= f.input :birth_date,  as: :string, input_html: {data: {provide:'datepicker' },class: 'date'}%>
		</div>
	</div>

   <div class="row">
		<div class="col-md-5">
			<%= f.input :cpf, input_html: {class: 'cpf'}%>
		</div>
		<div class="col-md-1">
			<%= f.input :teacher%>
		</div>
	</div>
   <div class="row">
   		<div class="col-md-5">
			<%= f.input :email%>
		</div>
		 		<div class="col-md-3">
			<%= f.input :phone, input_html: {class: 'phone'}%>
		</div>
   </div>
